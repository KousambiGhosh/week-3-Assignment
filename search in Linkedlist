class Node:
    def __init__(self, data):
        self.data = data
        self.next = None

class LinkedList:
    def __init__(self):
        self.head = None

    def insert_at_end(self, data): 
        new_node = Node(data)
        if not self.head:
            self.head = new_node
            return
        cur = self.head
        while cur.next:
            cur = cur.next
        cur.next = new_node

    def search(self, key):
        cur = self.head
        position = 1
        found = False

        while cur:
            if cur.data == key:
                print(f"Value {key} found at position {position}")
                found = True
            cur = cur.next
            position += 1

        if not found:
            print(f"Value {key} not found in the list.")

    def display(self):
        cur = self.head
        while cur:
            print(cur.data, end=" -> ")
            cur = cur.next
        print("None")


ll = LinkedList()
n = int(input("How many nodes to insert? "))
for _ in range(n):
    v = int(input("Enter value: "))
    ll.insert_at_end(v)

print("\nCurrent Linked List:")
ll.display()

key = int(input("\nEnter value to search: "))
ll.search(key)
